<time_limit><![CDATA[1]]></time_limit>
<memory_limit><![CDATA[128]]></memory_limit>

<description><![CDATA[<p><style type="text/css">p { margin-bottom: 0.21cm; }</style></p>
<p style="margin-bottom: 0cm;"><font color="#000000">输入两个正整数<font>m</font>和<font>n</font>，求其最大公约数和最小公倍数。</font></p>
<p></p>]]></description>
<input><![CDATA[]]></input> 
<output><![CDATA[]]></output>
<sample_input><![CDATA[2 3]]></sample_input>
<sample_output><![CDATA[1
6]]></sample_output>
<test_input><![CDATA[135
999
]]></test_input>
<test_output><![CDATA[27
4995
]]></test_output>
<hint><![CDATA[]]></hint>
<source><![CDATA[]]></source>
<solution language="C"><![CDATA[#include"stdio.h"
int main()
{
  int a,b,m,n,temp;
  //printf("请输入两个正整数:\n");
  scanf("%d%d",&m,&n);
  if(m<n)
   {temp=m;
    m=n;
    n=temp;
   }
  a=m;b=n;
  while(b!=0)
   {temp=a%b;
    a=b;
    b=temp;
   }
   printf("%d\n",a);
   printf("%d\n",m*n/a);
}
]]></solution>
